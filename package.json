{
  "name": "@twitchapis/twitch.js",
  "version": "1.1.0",
  "description": "twitch.js is a powerful Node.js module that allows you to easily interact with the TwitchTV making easy the way to make a TwitchTV bot, for a custom chat overlay for you OBS, or a moderation bot for you chat, or you just want a easy interface to TwitchTV.",
  "keywords": [
    "nodejs",
    "twitch",
    "twitchtv",
    "twitchirc",
    "twitch-api",
    "bot",
    "chat",
    "message"
  ],
  "main": "./src/index",
  "types": "./types/index.d.ts",
  "scripts": {
    "eslint": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "prettier": "prettier --write \"./**/*.{md,jsx,json,html,css,js,yml}\"",
    "prettier-check": "prettier --check \"./**/*.{md,jsx,json,html,css,js,yml}\"",
    "test": "node ./tests/testerv2.js",
    "testOld": "node ./tests/tester.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twitchapis/twitch.js.git"
  },
  "author": "TwitchApis Team",
  "contributors": [
    "LoboMetalurgico",
    "Space_Interprise"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twitchapis/twitch.js/issues"
  },
  "homepage": "https://twitch.js.org",
  "engines": {
    "node": ">=10.x"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "ws": "^8.7.0"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "prettier": "^2.6.2"
  }
}
