{
  "name": "@twitchapis/twitch.js",
  "version": "2.0.0-beta.7",
  "description": "Twitch.JS is a Nodejs library made with typescript to interact with the Twitch API in a simple and easy way.",
  "keywords": [
    "nodejs",
    "twitch",
    "twitchtv",
    "twitchirc",
    "twitch-api",
    "bot",
    "chat",
    "message"
  ],
  "type": "commonjs",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npx tsc --build",
    "prepack": "npm run build",
    "lint": "eslint ./src --fix",
    "test": "node ./tests/tester.js",
    "genDocs": "npx typedoc --options typedoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/promisependingapis/twitch.js.git"
  },
  "author": "PromisePendingApis Team",
  "contributors": [
    "LoboMetalurgico",
    "Space_Interprise"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/promisependingapis/twitch.js/issues"
  },
  "homepage": "https://twitch.js.org",
  "engines": {
    "node": ">=18.18"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@promisepending/logger.js": "^1.1.1",
    "@types/node": "^22.14.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.0.0",
    "typedoc": "^0.28.2",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "typescript": "^5.8.3"
  }
}
